        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Store.View"
        mc:Ignorable="d"
        Title="Statistics" Height="450" Width="800">
    <Grid>
        <Label Content="Статистика" HorizontalAlignment="Left" Margin="362,43,0,0" VerticalAlignment="Top" Height="41" Width="99"/>
        <Button Content="Профиль" HorizontalAlignment="Left" Margin="715,43,0,0" 
                VerticalAlignment="Top" Command="{Binding ProfileCommand}"
                Width="70" Height="30" Background="White"/> <!--Click="Profil"-->
        <Button Content="Товары" HorizontalAlignment="Left" Margin="715,97,0,0" 
                VerticalAlignment="Top" Command="{Binding ProductCommand}"
                Width="70" Height="30" Background="White "/> <!--Click="Prosucts"-->

    </Grid>
</Window>
